<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ui.logements.LogementController">
   <children>
      <AnchorPane fx:id="ancher" prefHeight="2000.0" prefWidth="2000.0" styleClass="root" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox style="-fx-background-color: #000;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox alignment="CENTER" style="-fx-background-color: #000;">
                     <children>
                        <HBox alignment="CENTER" prefHeight="28.0" prefWidth="860.0" spacing="12.0" style="-fx-background-color: #171B23;">
                           <children>
                              <JFXButton fx:id="buttonShowChart" alignment="CENTER" buttonType="RAISED" contentDisplay="CENTER" prefHeight="45.0" prefWidth="45.0" style="-fx-background-color: #FFFFFF;" text=" ">
                                 <graphic>
                                    <FontAwesomeIconView fx:id="sss" fill="WHITE" glyphName="PIE_CHART" size="24" styleClass="iconPrimary" />
                                 </graphic>
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                              </JFXButton>
                              <JFXButton fx:id="btnReload" alignment="CENTER" buttonType="RAISED" contentDisplay="CENTER" prefHeight="45.0" prefWidth="45.0" style="-fx-background-color: #FFFFFF;" text=" ">
                                 <graphic>
                                    <FontAwesomeIconView fill="WHITE" glyphName="REFRESH" size="24" styleClass="iconPrimary" />
                                 </graphic>
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                              </JFXButton>
                              <JFXButton fx:id="btnInsert" alignment="CENTER" buttonType="RAISED" contentDisplay="CENTER" prefHeight="45.0" prefWidth="45.0" style="-fx-background-color: #FFFFFF;" text=" ">
                                 <graphic>
                                    <FontAwesomeIconView fill="WHITE" glyphName="PLUS" size="24" styleClass="iconPrimary" />
                                 </graphic>
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                              </JFXButton>
                              <TextField fx:id="textFieldIdSearch" prefHeight="35.0" prefWidth="250.0" promptText="ID de logement" style="-fx-background-color: #282E35;" styleClass="text-light">
                                 <font>
                                    <Font name="System Bold" size="14.0" />
                                 </font></TextField>
                              <Button fx:id="buttonSearchId" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="45.0" prefWidth="45.0" style="-fx-background-color: #fff;" text=" ">
                                 <graphic>
                                    <FontAwesomeIconView glyphName="SEARCH" size="24" />
                                 </graphic>
                              </Button>
                              <TextField fx:id="textFieldMomDeLogmement" prefHeight="35.0" prefWidth="250.0" promptText="nom de logement" style="-fx-background-color: #282E35;" styleClass="text-light">
                                 <font>
                                    <Font name="System Bold" size="14.0" />
                                 </font></TextField>
                              <Button fx:id="buttonSearchNom" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="45.0" prefWidth="45.0" style="-fx-background-color: #fff;" text=" ">
                                 <graphic>
                                    <FontAwesomeIconView glyphName="SEARCH" size="24" />
                                 </graphic>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                     <VBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </VBox.margin>
                     <padding>
                        <Insets top="5.0" />
                     </padding>
                  </HBox>
                  <TableView fx:id="table" prefHeight="2000.0" prefWidth="2000.0" styleClass="root">
                    <columns>
                      <TableColumn fx:id="col_image" prefWidth="75.0" text="image" />
                      <TableColumn fx:id="col_id" prefWidth="75.0" text="id" />
                        <TableColumn fx:id="col_titre" prefWidth="75.0" text="titre" />
                        <TableColumn fx:id="col_description" prefWidth="75.0" text="description" />
                        <TableColumn fx:id="col_address" prefWidth="75.0" text="Address" />
                        <TableColumn fx:id="col_Equpment" prefWidth="75.0" text="Equpment" />
                        <TableColumn prefWidth="75.0" text="Activation">
