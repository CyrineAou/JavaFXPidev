                              <TableColumn fx:id="col_btn_activation" prefWidth="75.0" text=" " />
                              <TableColumn fx:id="col_btn_show" prefWidth="75.0" text=" " />
                              <TableColumn fx:id="col_btn_update" prefWidth="75.0" text=" " />
                              <TableColumn fx:id="col_btn_delete" prefWidth="75.0" text=" " />
                           </columns>
                        </TableColumn>
                    </columns>
                     <stylesheets>
                        <URL value="@../../../resources/them/css/rootStyle.css" />
                        <URL value="@../../../resources/them/css/mainMaterial.css" />
                        <URL value="@../../../resources/them/css/bootstrapFx.css" />
                     </stylesheets>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </VBox>
         </children>
         <stylesheets>
            <URL value="@../../../resources/them/css/mainMaterial.css" />
            <URL value="@../../../resources/them/css/bootstrapFx.css" />
         </stylesheets>
